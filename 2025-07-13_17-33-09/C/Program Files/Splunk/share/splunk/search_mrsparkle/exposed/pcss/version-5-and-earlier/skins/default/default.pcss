.ButtonSwitcher ul li.disabled a,
.ButtonSwitcher ul li.selected a {
	cursor:default;
}


.ButtonSwitcher ul li {
    border-color: $white;
    border-radius: 3px;
}

.ButtonSwitcher ul li:hover {
    background-color: #e2e2e2;

}.ButtonSwitcher ul li.disabled:hover {
    background-color: transparent;
    cursor:default;
}

.ButtonSwitcher ul li.selected, .ButtonSwitcher ul li.selected:hover {
    background-color: #e2e2e2;
    color: $textGray;
}

/* Link Switcher */
.LinkSwitcher a {
    font-size: $fontSize;
}
.LinkSwitcher ul li.selected a {
    color:$gray30;
    font-weight: bold;
    text-decoration:none;
}

/* ShowHideHeader */
.ShowHideHeader div.secondary {
    background-image: none;
}
.ShowHideHeader h2 span.splIcon-triangle {
    background-position: -67px -367px;
}
.ShowHideHeader div.secondary h2 {
    font-size: $fontSizeSmall;
    font-weight: normal;
}
.ShowHideHeader div.secondary h2 span.splIcon-triangle {
    background-position: -87px -367px;
}
.ShowHideHeader div.secondary h2:hover {
    text-decoration:underline;
}
.ShowHideHeader h2.closed span.splIcon-triangle {
    background-position:  -67px -407px;
}
.ShowHideHeader div.secondary h2.closed span.splIcon-triangle {
    background-position: -87px -407px ;
}

/* TabSwitcher */
.TabSwitcher {
    background-color: #bdbdb7;
}
.TabSwitcher ul li {
    background: #666 url(/static/img/skins/default/tab_switcher_rounded_corners.gif) no-repeat 0 -68px;
}
.TabSwitcher ul li a {
    font-size: $fontSize;
    background: #666 url(/static/img/skins/default/tab_switcher_rounded_corners.gif) no-repeat right -102px;
    color: $white;
}
.TabSwitcher ul li.selected {
    background-color: $white;
    background: $white url(/static/img/skins/default/tab_switcher_rounded_corners.gif) no-repeat 0 0;
}
.TabSwitcher ul li.selected a {
    color: $gray30;
    background: $white url(/static/img/skins/default/tab_switcher_rounded_corners.gif) no-repeat right -34px;
}

/* progress bar */

.JobProgressIndicator .splBarGraph {
    background-image:url(/static/img/skins/default/overlay_innershadow_4.png);
}

.JobProgressIndicator .splBarGraphBar {
    background-image:url(/static/img/skins/default/overlay_gradient_4.png);
}

/* PAYWALL */
.section-paywall {
    padding: 10px 20px 20px;
    min-height: 500px;
    font-family: $fontFamily;
}

.section-paywall .section-title {
    margin: 0;
    font-size: 24px;
    font-weight: 200;
    line-height: 29px;
    color: $gray30;
    text-rendering: optimizelegibility;
}
.section-paywall #paywall-content {
    margin: 10px 0;
    font-size: $fontSize;
    line-height: $lineHeight;
}
/* one off button override for consistancy */
.section-paywall a.splButton-primary {
    line-height: $lineHeight;
    vertical-align: middle;
    padding: 4px 12px;
    border-radius:$interactiveBorderRadius;
    background-color: $brandColor;
    background-repeat: repeat-x;
    border-color: #4E802A #426D24 #32521B;
    border-left: 1px solid #426D24;
    border-right: 1px solid #426D24;
    border-style: solid;
    border-width: 1px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.08);
    color: $white;
    text-shadow: 0 -1px 0 rgba(51, 51, 51, 0.7);
    display: inline-block;
    height: auto;
}
.section-paywall a.splButton-primary ,
.section-paywall a.splButton-primary span
{
    min-width: 0;
    float: none !important;
}
.section-paywall a.splButton-primary span
{
    display: inline !important;
}
