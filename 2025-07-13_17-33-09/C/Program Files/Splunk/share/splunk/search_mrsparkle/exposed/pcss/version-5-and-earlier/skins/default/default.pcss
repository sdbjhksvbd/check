/*
 *  Glorious Splunk Skin
 */

/*************************************************************************************************/
/*  TYPOGRAPHY                                                                                   */
/*************************************************************************************************/
@mixin define-font 'Proxima Nova', proxima-bold-webfont.woff, woff, bold;                     /*  Bold */
@mixin define-font 'Proxima Nova', proxima-regular-webfont.woff;                              /*  Regular */
@mixin define-font 'Proxima Nova', proxima-semibold-webfont.woff, woff, $fontWeightSemiBold;   /*  semi-bold */
@mixin define-font 'Inconsolata', inconsolata-regular.woff, woff;                               /*  Monospace */
@mixin define-font 'Architects Daughter', ArchitectsDaughter.ttf, truetype;
@mixin define-font 'Splunk Icons', splunkicons-regular-webfont.woff;  /*  Splunk Icons :  unicode-range: U+00-FF; */

body {
    font-family: $fontFamily;
    font-size: $fontSize;
    color: $gray30;
}
input, textarea, select, optgroup {
    font-family: $fontFamily;
    font-size: $fontSize;
    color: $textColor;
}
select {
    @mixin interactive;
    @mixin button-states;
    height: 32px;
    cursor: pointer;
    appearance: none;
    background-image: url(data:image/svg+xml;base64,);
    background-repeat: no-repeat;
    background-position: right;
    padding: 2px 25px 2px $spacingHalf;
    text-overflow: ellipsis;
    &:hover {
        color: $linkColor;
    }
    &:focus {
        outline: none !important;
        outline-style: none;
    }
    &:-moz-focusring {
        color: transparent;
        text-shadow: 0 0 0 #000;
    }
    &::-ms-expand {
        display: none;
    }
}


/*************************************************************************************************/
/*  HEADINGS                                                                                     */
/*************************************************************************************************/
h1, h2, h3, h4 {
    margin: calc($lineHeight / 2) 0;
    font-family: $headingsFontFamily;
    font-weight: $headingsFontWeight;
    line-height: $headingsLineHeight;
    color: $headingsColor;
    text-rendering: optimizelegibility; /*  Fix the character spacing for headings */
}

h1 {
    font-size: $fontSizeXXLarge;
    font-weight: bold;
}
h2 {
    font-size: $fontSizeXLarge;
}

h3 {
    font-size: $fontSizeLarge;
}
h4 {
    font-size: $fontSize;
    color: $textColor;
    font-weight: bold;
    margin: 0;
}

/* font styles */
.splFont-mono, .SearchBar label {
    font-family: $monoFontFamily;
}

.SearchBar textarea, .SearchBar label {
    font-size: $fontSize;
    box-shadow: none;
}
.SearchBar textarea:focus {
    outline: 0;
}


/*************************************************************************************************/
/*  LINK COLORS                                                                                  */
/*************************************************************************************************/
a {
    @mixin link;

    &.disabled {
        color: $textGray;
    }
}


/*************************************************************************************************/
/*  APPLICATION HEADER                                                                           */
/*************************************************************************************************/
/* app header wrapper */
/* - this can be used to set a background for the entire header area.  */
.appHeaderWrapper {
    background: $navbarBackground;
    margin: 0 !important;
    min-height: $navbarHeight;

    /*
    use this to display the name of the app.
    use line-height to adjust alignment with logo. if
    if the name of the app is in the logo,  set this to display: none;
    */
    h1 {
        line-height: 43px;
    }
}

/* change the app logo here.  set the height/width for your image, as well as the path to the image */
.appLogo {
    height: 43px;
    width: 80px;
    background: url(/static/img/skins/default/splunk_logo_black.png) no-repeat 0 0;
}


/*************************************************************************************************/
/*  BACKGROUND COLOR                                                                             */
/*************************************************************************************************/
/* default page color */
body {
    background-color: $bodyBackgroundColor;
}

.splTemplate-search, .splTemplate-dashboard {
    background-color: $bodyBackgroundColor;
}

.splBackground-default, .resultsArea,
.reportSecondPanel, .reportThirdPanel, .sidebarCollapsed,
.SearchBar .saTypeaheadWrapper {
    background-color: $white;
}

/* primary background - applies to search controls and primary action panels */
.saHelpWrapper {
    background-color: $gray98;
}

/* secondary background - sidebar, other panels */
.splBackground-secondary, .sidebarExpanded {
    background: $bodyBackgroundColor;
}

/* Specific overrides */
.layoutCellInner .ResultsHeader .splHeader, .layoutCellInner .ResultsHeader .splHeader-secondary {
    background: transparent none;
}


/*************************************************************************************************/
/*  HEADERS                                                                                      */
/*************************************************************************************************/
.splHeader-primary {
    border-top: $border;
}

.splHeader-secondary {
    border-top: $border;
    background-image: $bodyBackgroundColor;
}
/* navigation bar */
.splHeader-navigation {
    background-image: none;
}

.splHeader h2 a {
    font-weight:normal;
}

/* Specific overrides */
.TitleBar .splHeader, .FieldPickerPopup .splHeader-primary {
    background-image: none;
}

.FlashTimeline {
    background: $white url(/static/img/skins/default/overlay_bottomgradient_7.png) repeat-x bottom center;

    .splHeader-primary {
        background: $white url(/static/img/skins/default/overlay_topgradient_7.png) repeat-x;
    }
}

.DisableRequiredFieldsButton {
    background: $bodyBackgroundColor url(/static/img/skins/default/overlay_topgradient_32.png) repeat-x;
}

.splView-flashtimeline .ResultsHeader .splHeader-primary {
    background: $bodyBackgroundColor url(/static/img/skins/default/overlay_topgradient_32.png) repeat-x;
    border-color: #a4a4a4;
    padding-top: 5px;
}

.splView-flashtimeline .ResultsHeader .splHeader-primary h2 {
    background-color: $white;
    border-top-left-radius: 6px;
}


/*************************************************************************************************/
/*  BORDERS                                                                                      */
/*************************************************************************************************/
* {
    border-color: $borderColor;
}

div.sidebarCollapsed .sidebarControl {
    border-radius: 0 0 5px 0;
    background: $bodyBackgroundColor url(/static/img/skins/default/overlay_topgradient_32.png) repeat-x;
    border-top: 1px solid #A4A4A4;
}

div.sidebar .FieldPicker {
    border-color: #a4a4a4;
}


/*************************************************************************************************/
/*  DASHBOARDS                                                                                   */
/*************************************************************************************************/
/* dashboard headers */
.splHeader-dashboard {
    background-image: none;
    background-color: transparent;

    body& {
        background-color: $bodyBackgroundColor;
    }
}
.splHeader-dashboard h2,
.dashboardContent .ServerSideInclude h2,
.dashboardContent .GenericHeader h3 {
    font-size: $fontSize;
    color: $brandColor;
    font-weight:bold;
    background-color: transparent;
}
/* rounded box for dashboard modules */
.dashboardCell {
    position: relative;
    background: $white;
    border-style: solid;
    border-width: 1px;
    border-radius: 2px;
}

.dashboardContent {
    .SimpleResultsTableResults, .EventsViewer {
        background-image: none;
        background: transparent;
    }
}


/*************************************************************************************************/
/*  FORM ELEMENTS                                                                                */
/*************************************************************************************************/
fieldset legend {
    color: $brandColor;
    font-size: $fontSize;
    font-weight: bold;
    span {
        color: $black;
        font-size: 10px;
        font-weight: normal;
    }
}

input, textarea, select {
    font-family: $fontFamily;
    font-size: $fontSize;

    &:focus {
        @mixin focus-element;
    }
}
label {
    font-size: $fontSize;
    &.disabledLabel {
        color:$textDisabledColor;
    }
}
select option[disabled] {
    color: $textGray;
    box-shadow: none;
}
input.readonly {
    background-color: $gray60;
    box-shadow: none;
}
p.exampleText {
    color: $textGray;
    clear: both;

    em {
        font-style: normal;
    }
}
p.fieldsetHelpText {
    color: $textGray;
}
input[disabled]:not([type=checkbox]){
    background-color: $gray98;
    color: $gray30;
    box-shadow: none;
}
.splTextArea {
    textarea, > div {
        border: $borderDefault;
        padding: $spacingQuarter;
        box-sizing: border-box;

        &:focus {
            border-color: $focusColor;
        }
    }
}


/*************************************************************************************************/
/*  TABLES                                                                                       */
/*************************************************************************************************/
table.splTable {
    border-color: $tableBorderColor;
    th {
        border-color: $tableBorderColor;
        a {
            color: $textColor;
        }
    }
    td {
        border-color: $tableBorderColor;
    }
}
.empty_results {
    background: #eaeaea;
    border: 0px !important;
    font-size: $fontSize;
    font-weight: normal !important;
    padding: $spacingHalf !important;
    border-radius: $interactiveBorderRadius;
    color:$textGrayDark !important;
}

/* sorting */
.splSortNone, .splSortAsc, .splSortDesc {
    background-image: url(/static/img/skins/default/splIcons.gif);
    background-position: -67px -446px;
    background-repeat: no-repeat;
    cursor: pointer;
}
.splSortDesc {
    background-position: -67px -365px;
    &:hover {
        background-position: -67px -385px;
    }
}
.splSortAsc {
    background-position: -67px -385px;
    &:hover {
        background-position: -67px -366px;
    }
}


/*************************************************************************************************/
/*  GLOBAL ELEMENTS                                                                              */
/*************************************************************************************************/
.splPipe {
    color: $textGray;
}
.splDivider {
    border-bottom-style: solid;
    border-bottom-width: 1px;
}
#loading  {
    background-color:$brandColor;
    color:$white;
}  /* I smell a refactor here... */
#loadingmessage {
    font-size:18px;
    background: url(/static/img/skins/default/loading_white.gif) no-repeat 0 0;
}
.popupLoading {
    background: url(/static/img/skins/default/loading_white.gif) no-repeat 0 20px;
    font-size:18px;
}
.mouseoverHighlight {
    &, & td {
        background-color:$warningColorL40;
    }
}

.searchFieldGhost {
    border-color: $gray30;
}
.widgeterror {
    color: $errorColor;
    font-weight: bold;
}

.resultStatusMessage {
    color:$textGrayDark;
}


/*************************************************************************************************/
/*  PERCENTAGE BAR GRAPH                                                                         */
/*************************************************************************************************/
.splBarGraph {
    background: $bodyBackgroundColor;
}
.splBarGraphBar {
    background: $brandColor url(/static/img/skins/default/overlay_gradient_28.png) repeat-x;
}
.graphLoading {
    padding-bottom: 5px;
}


/*************************************************************************************************/
/*  POPUPS                                                                                       */
/*************************************************************************************************/
.popupContainer {
    z-index: 10000;
    border-color: $borderColor;
    box-shadow: $overlayBoxShadow;

    /* fieldpicker popup */
    .fieldLayers & {
        border-color: $borderColor;
    }
}
.popupContent {
    .schedulePdfPopup & iframe {
        border: none;
    }
    .wizardPopup & iframe {
        width: 400px;
        border: none;
        display: block;
    }
    .wideTreeviewPopup & iframe {
        width: 700px;
        height: 433px;
        border: none;
        display: block;
    }
    .panelEditorPopup & {
        min-width: 340px;
        iframe {
            width: 340px;
            border: none;
            display: block;
        }
    }
}

/* Field Value popup-specific styles */
.fieldValuePopup {
    border-color: $borderColor;
    box-shadow: none;
    border: none;

    .fieldValuePopupInner {
        border: $borderDefault;
        background-image:url(/static/img/skins/default/bg_reversegradient_28.png);

    }
    h3 em {
        font-size: $fontSizeSmall;
    }
    table {
        th.fieldName {
            font-weight: normal;

            tr:first-child & {
                font-weight: bold;
            }
        }
        td, th {
            color: $gray30;
            border-bottom-style: dotted;
            border-bottom-width: 1px;
        }
        tr, th {
            border-bottom-style: solid;
            border-bottom-width: 1px;
        }
        tr.fieldNameHeaderRow td {
            font-weight: bold;
            color: $black;
            border-bottom: none;
        }
    }
    p.reportLinks,
    div.reportLinks {
        border-radius: $interactiveBorderRadius;
    }
}

.pdfPopup {
    background: $white;
}

/* popup header bar */
.splHeader-popup {
    background: $white;
    border-bottom: $borderDefault;

    h2, h1 {
        font-size: $fontSize;
        font-weight: normal;
    }
    &, h2 {
        cursor: move;
    }
    &, h1 {
        cursor: move;
    }
}

/* popup content */
.popupContent {
    background-color: $white;
    position: relative;

    /*iframe loading*/
    .popup-loading {
        width: 100%;
        height: 100%;
        position: absolute;
        top: 0;
        text-indent: -1000em;
        direction: ltr;
        background: $white url('/static/img/skins/default/loading_white.gif') no-repeat center center;
    }
}
.error {
    .popupContent &, .wizard & {
        font-size: $fontSize;
        background: $errorColor;
        margin: $spacingHalf $spacingHalf 0;
        border-radius: $interactiveBorderRadius;
    }
}

/* popup footer (button container) */
.popupFooter {
    border-top: $borderDefault;
    background: $white;
}

.wizard {
    .scheduledView {
        .error,
        .info {
            margin: 0px 0px $spacingHalf;
        }
        .disabled {
            cursor: not-allowed;
        }
    }
}


/*************************************************************************************************/
/*  OVERLAYS AND SHADOWS                                                                         */
/*************************************************************************************************/
.splOverlay, .splOverlay-white {
    background-color: $black;
    opacity:0.5;
    filter:alpha(opacity=50);
}
.splOverlay-white {
    background-color:$white;
}
.splShadow {
    background: url(/static/img/skins/default/shadow_soft.png) no-repeat bottom right;
    border-top-right-radius:17px;
    border-bottom-left-radius:17px;
}


/*************************************************************************************************/
/*  BUTTONS                                                                                      */
/*************************************************************************************************/
.splButton-primary,
.splButton-secondary,
.splButton-tertiary,
.splRadio-control {
    @mixin base-input $buttonPadding, auto;
    @mixin button-padding;
    cursor: pointer;
    font-size: $fontSize;
    font-family: $fontFamily;
    border-radius: $interactiveBorderRadius;
    @mixin interactive;

    button& span {
        position: relative;
        bottom: 1px;
    }
}

.splButton-primary {
    @mixin button-padding-primary;
    font-weight: $fontWeightSemiBold;
    border: none;

    @mixin interactive-primary;
    @mixin button-states interactive-primary-hover, interactive-primary-active, interactive-primary-disabled, interactive-primary $primaryBackgroundColor;

    span.splMenuIcon {
        background-position: 0 -300px;
    }
}
.splButton-secondary,
.splButton-tertiary,
.splRadio-control {
    border: $interactiveBorder;
    @mixin button-states;
    span.splMenuIcon {
        background-position: -13px -300px;
    }
    &.selected, &.s-active {
        @mixin interactive-hover;
    }
}
.splButton-disabled {
    &, &:hover, &:focus {
        background: $interactiveBackgroundColorDisabled;
        filter: none;
        border-color: $borderColor;
        cursor: not-allowed;
        color: $textGray;
        text-shadow: none;
        opacity: 0.65;
    }
    .splButtonIcon {
        opacity:0.5;
    }
}


/*************************************************************************************************/
/*  BUTTON GROUPS                                                                                */
/*************************************************************************************************/
.splButtonGroup {
    .splButton-primary,
    .splButton-secondary,
    .splButton-tertiary {
        border-radius: 0;
        margin:0;

        &:first-child {
            border-radius: $inputBorderRadiusLeft;
        }
        &:last-child {
            border-radius: $inputBorderRadiusRight;
        }
    }
    .splButton-disabled {
        border-left-color: #bbb;
        cursor:not-allowed;
    }
}


/* Iconic Links
---------------------------------*/
.splIconicLinkIcon, .splButtonIcon, span.splMenuIcon {
    background-image: url(/static/img/skins/default/sprite_button_icons.png);
    background-position: 0 0;
}

.splIconicLinkIcon {
    margin-top:2px;
}
.splIconicLinkLabel {
    font-size: $fontSizeSmall;
}
.splIcon-export {
    background-position: -26px -660px;
}
.splIcon-options {
    background-position: -26px -680px;
}
.splIconicLinkDisabled  {
    background-image: none;
    color: $textGray;

    .splIconicLinkIcon {
        opacity: 0.45;
        color: $textGray;
        border-radius: 3px;
    }
    .splIcon-export {
        background-position: -13px -660px;
    }
}


/* Splunk search button */
input.searchButton {
    @mixin search-icon-background;
    background-position: 3px 2px;
    width: 32px !important;
    height: 32px !important;
    cursor: pointer;
    color: $textColor;
    border: $interactiveBorder;
    border-radius: $inputBorderRadiusRight;
    @mixin interactive;
    @mixin button-states;
}

/* button wrapper */
.splButtonWrapper {
    border-top: $border;
}

/* icons
---------------------------------*/
.splIcon {
    background-image: url(/static/img/skins/default/splIcons.gif);
    background-color: $gray60;
}

/*-- external link icon --*/
.spl-icon-external-link-xsm {
    vertical-align: middle;
    white-space: nowrap;
    &:before {
        display: inline-block;
        padding: 0 4px;
        content: "\EC13";
        font-family: 'Splunk Icons';
        vertical-align: middle;
    }
}
.spl-icon-external-link-xsm.inline-icon {
	margin-left: 0px;
}

/*-- sidebar collapse icon --*/
.splIcon-sidebar-open .splIconicLinkIcon { background-position: -26px -320px; }
.splIcon-sidebar-closed .splIconicLinkIcon { background-position: -26px -340px; }

/*-- linear and log scale buttons --*/
div.FlashTimeline a.linLogToggle {
    color:$black;
}

div.FlashTimeline a.linLogToggle:focus {
   background-color: $gray80;
}

div.FlashTimeline a.linLogToggle .splIcon-triangle-4-s {
	background-position: -67px -367px;
}



/*-- clear buttons --*/
.splIcon-clear {    border-radius: 5px;
    background-position: 0px 0px;
}
.splIcon-clear:hover { background-position: -20px 0px; }

/*-- close icons --*/
.splIcon-close { background-position: 0px 0px; }
.splIcon-close:hover { background-position: -20px 0px; }

/*-- arrow icons --*/
.splIcon-arrow-n, .splIcon-arrow-e, .splIcon-arrow-s, .splIcon-arrow-w {
    background-color: $gray60;
}

.splIcon-arrow-n { background-position: 0px -100px; }
.splIcon-arrow-e { background-position: 0px -140px; }
.splIcon-arrow-s { background-position: 0px -120px; }
.splIcon-arrow-w { background-position: 0px -160px; }
.splIcon-arrow-n:hover { background-position: -20px -100px; }
.splIcon-arrow-e:hover { background-position: -20px -140px; }
.splIcon-arrow-s:hover { background-position: -20px -120px; }
.splIcon-arrow-w:hover { background-position: -20px -160px; }

/*-- results view buttons --*/
.splIcon-events-list, .splIcon-events-table, .splIcon-results-table, .splIcon-results-chart {
    background-image: url(/static/img/skins/default/sprite_button_icons.png);
}
.splIcon-events-list { background-position: -26px -740px; }
.splIcon-events-table { background-position: -26px -760px; }
.splIcon-results-table { background-position: -26px -780px; }
.splIcon-results-chart { background-position: -26px -800px; }
.splIcon-events-list:hover { background-position: -39px -740px; }
.splIcon-events-table:hover { background-position: -39px -760px; }
.splIcon-results-table:hover { background-position: -39px -780px; }
.splIcon-results-chart:hover { background-position: -39px -800px; }

/* triangles */
/* Note: to separate color from implementation, we're using a numbering system to differentiate colors.
   1=grey,2=white,3=green,4=black,5=blue.  If the icon sprite changes, the number mapping to colors would be different */
.splIcon-triangle,
.splIcon-triangle-1-n, .splIcon-triangle-1-s, .splIcon-triangle-1-e, .splIcon-triangle-1-w,
.splIcon-triangle-2-n, .splIcon-triangle-2-s, .splIcon-triangle-2-e, .splIcon-triangle-2-w,
.splIcon-triangle-3-n, .splIcon-triangle-3-s, .splIcon-triangle-3-e, .splIcon-triangle-3-w,
.splIcon-triangle-4-n, .splIcon-triangle-4-s, .splIcon-triangle-4-e, .splIcon-triangle-4-w,
.splIcon-triangle-5-n, .splIcon-triangle-5-s, .splIcon-triangle-5-e, .splIcon-triangle-5-w {
    background-color:transparent;
}
.splIcon-triangle-large {
    background-color:transparent;
}

/* grey */
.splIcon-triangle-1-n { background-position: -7px -386px; }
.splIcon-triangle-1-s { background-position: -7px -367px; }
.splIcon-triangle-1-e { background-position: -7px -407px; }
.splIcon-triangle-1-w { background-position: -7px -427px; }
/* white */
.splIcon-triangle-2-n { background-position: -27px -386px; }
.splIcon-triangle-2-s { background-position: -27px -367px; }
.splIcon-triangle-2-e { background-position: -27px -407px; }
.splIcon-triangle-2-w { background-position: -27px -427px; }
/* green */
.splIcon-triangle-3-n { background-position: -47px -386px; }
.splIcon-triangle-3-s { background-position: -47px -367px; }
.splIcon-triangle-3-e { background-position: -47px -407px; }
.splIcon-triangle-3-w { background-position: -47px -427px; }
/* black */
.splIcon-triangle-4-n { background-position: -67px -386px; }
.splIcon-triangle-4-s { background-position: -67px -367px; }
.splIcon-triangle-4-e { background-position: -67px -407px; }
.splIcon-triangle-4-w { background-position: -67px -427px; }
/* blue */
.splIcon-triangle-5-n { background-position: -87px -386px; }
.splIcon-triangle-5-s { background-position: -87px -367px; }
.splIcon-triangle-5-e { background-position: -87px -407px; }
.splIcon-triangle-5-w { background-position: -87px -427px; }

/* state interaction
_________________________________*/

.fatal, .error, .warn, .info, .persistent {
    font-weight: bold;
}

.fatal, .error {
    background-color: $errorColor;
    color: $white;
}
.warn, .persistent {
   background-color: $warningColor;
   color: $white;
}
.info {
   background-color: $infoColor;
   color: $white;
}


.fatal .remove, .error .remove, .warn .remove, .info .remove, .persistent .remove {
    border-radius: $interactiveBorderRadius;
}

.fatal .remove, .error .remove {
    background-color: $errorColor;
}
.warn .remove, .persistent .remove {
   background-color: $warningColor;
}
.info .remove {
   background-color: $infoColor;
}


/* menu classes
---------------------------------*/

.splMenu {
    font-size: $fontSizeSmall;
    font-family: Arial, Helvetica, sans-serif;
}

/* primary menu - white */
.splMenu-primary, .splMenu-primary a {
    color: $gray30;
}
.splMenu-primary li.disabled a {
    color: $textGray;
}
.splMenu-primary ul {
    background-color: $white;
}
.splMenu-primary .actionsMenuDivider {
}

/* primary menu hover styles */
.splMenu-primary li:hover {
    background: #ddd;
}
.splMenu-primary ul li.htmlBlock:hover {
    background-color: transparent;
}

/* secondary menu - black */
.splMenu-secondary, .splMenu-secondary a {
    color: $gray80;
}
.splMenu-secondary li.disabled a {
    color: $textGray;
}
.splMenu-secondary ul {
    background-color: $black;
    border-color: $gray30;
}

/* primary menu hover styles */
.splMenu-secondary li:hover {
    background-color: #7b9059;
}
.splMenu-secondary a:hover {
    color: $white;
}
.splMenu-secondary ul li.htmlBlock:hover {
    background-color: transparent;
}

