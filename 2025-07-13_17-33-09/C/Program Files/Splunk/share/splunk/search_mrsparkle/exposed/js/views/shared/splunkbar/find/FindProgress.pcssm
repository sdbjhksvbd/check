

/*  Find Results */
.view {
    composes: view from '../../MenuDialog.pcssm';
    background-color: $popupBackgroundColor;
    width: 360px;
}

.list {
    composes: menu from '../../MenuDialog.pcssm';
}

.arrow {
    composes: arrow from '../../MenuDialog.pcssm';
}

.primaryLink {
